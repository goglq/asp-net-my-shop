{
  "ConnectionStrings": {
    "Default": "Data Source=myapp.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ProductServiceOptions": {
    "DailyOverpricePercentage": 50,
    "AndroidSalePercentage": 10,
    "IPhoneOverpricePercentage": 50
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "CorsSettings": {
    "Origins": ["https://localhost:7074","http://localhost:5074"],
    "Methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
    "Headers": ["*"]
  }
}
